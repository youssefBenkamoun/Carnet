<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/parent"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:padding="10dp">

    <TextView
        android:id="@+id/client"
        android:layout_width="157dp"
        android:layout_height="24dp"
        android:layout_marginStart="5dp"
        android:paddingTop="5dp"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.174"
        app:layout_constraintStart_toEndOf="@+id/clientLabel"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/prix"
        android:layout_width="157dp"
        android:layout_height="24dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="10dp"
        android:paddingTop="5dp"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.099"
        app:layout_constraintStart_toEndOf="@+id/prixLabel"
        app:layout_constraintTop_toBottomOf="@+id/product"
        app:layout_constraintVertical_bias="0.2" />

    <TextView
        android:id="@+id/product"
        android:layout_width="157dp"
        android:layout_height="24dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="10dp"
        android:paddingTop="5dp"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.108"
        app:layout_constraintStart_toEndOf="@+id/produitLabel"
        app:layout_constraintTop_toBottomOf="@+id/client" />

    <TextView
        android:id="@+id/prixLabel"
        android:layout_width="38dp"
        android:layout_height="26dp"
        android:layout_marginStart="90dp"
        android:layout_marginTop="10dp"
        android:paddingTop="5dp"
        android:text="Date :"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/produitLabel"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/produitLabel"
        android:layout_width="38dp"
        android:layout_height="24dp"
        android:layout_marginStart="90dp"
        android:layout_marginTop="10dp"
        android:paddingTop="5dp"
        android:text="Etat :"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/clientLabel" />

    <TextView
        android:id="@+id/ids"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="4dp"
        android:text="TextView"
        android:visibility="gone"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/clientLabel"
        android:layout_width="38dp"
        android:layout_height="25dp"
        android:layout_marginStart="90dp"
        android:paddingTop="5dp"
        android:text="Prix :"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="60dp"
        android:layout_height="58dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_baseline_monetization_on_24" />

    <ImageView
        android:id="@+id/delete"
        android:layout_width="35dp"
        android:layout_height="38dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.493"
        app:srcCompat="@drawable/ic_baseline_delete_24" />

</androidx.constraintlayout.widget.ConstraintLayout>